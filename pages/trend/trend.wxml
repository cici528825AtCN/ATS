<!--pages/trend/trend.wxml-->
<view class='container'>
  <view class="title"> 
    {{chartTitle}}
  </view>
  <canvas canvas-id='ATScanvas' class='canvas' bindtouchstart='touchHandler'></canvas>
  <map 
    style='width: 100%; height: 200px' 
    latitude = "{{latitude}}" 
    longitude='{{longitude}}' 
    markers = "{{markers}}" 
    covers='{{covers}}'></map>
</view>

